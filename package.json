    "animate.css": "^4.1.1",
    "axios": "^1.10.0",
    "classnames": "^2.5.1",
    "clsx": "^2.1.1",
    "firebase-admin": "^13.4.0",
    "framer-motion": "^12.23.5",
    "gsap": "^3.13.0",
